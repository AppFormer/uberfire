<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui">

  <g:SimplePanel>
    <b:PanelGroup ui:field="accordion">
      <b:Panel>
        <b:PanelHeader dataToggle="COLLAPSE" text="Tree Nodes" ui:field="header"/>
        <b:PanelCollapse ui:field="collapseOne" in="true">
          <b:PanelBody>
            <g:SimplePanel ui:field="categoryTreeNodes"/>
          </b:PanelBody>
        </b:PanelCollapse>
      </b:Panel>
    </b:PanelGroup>
  </g:SimplePanel>

</ui:UiBinder>