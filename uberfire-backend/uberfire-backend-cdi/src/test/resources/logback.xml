<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger - %msg%n</pattern>
    </encoder>
  </appender>

  <logger name="org.uberfire.backend.server.cdi" level="TRACE"/>

  <root level="INFO">
    <appender-ref ref="STDOUT"/>
  </root>
</configuration>